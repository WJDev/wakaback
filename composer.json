{
    "name": "WJDev/Wakaback",
    "type": "library",
    "description": "A simple utility to back up and display your wakatime activity",
    "keywords": [
        "WJDev",
        "wakaback",
        "wakatime"
    ],
    "homepage": "https://github.com/WJDev/wakaback",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Hall",
            "email": "ben@elanman.com",
            "homepage": "https://github.com/elanman",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "guzzlehttp/guzzle": "~6.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "WJDev\\Wakaback\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WJDev\\Wakaback\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
